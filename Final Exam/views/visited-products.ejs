    <div class="visited-products">
        <h1>Visited Products</h1>
        <ul>
            <% visitedProducts.forEach(product => { %>
                <li>
                    <a href="/products/<%= product._id %>"><%= product.name %></a>
                    - $<%= product.price %>
                </li>
            <% }) %>
        </ul>
    </div>